> {\n    dispatch({ type: \"BLUR\" });\n  };\n  const reset = () => {\n    dispatch({ type: \"RESET\" });\n  };\n\n  return {\n    enterValue : value,\n    hasError: enterValueIsInvalid,\n    onEnterValueChangeHandler,\n    onInputIsTouchedHandler,\n    reset,\n  };\n};\n\nexport default useInputPractice;\n"]},"metadata":{},"sourceType":"module"}