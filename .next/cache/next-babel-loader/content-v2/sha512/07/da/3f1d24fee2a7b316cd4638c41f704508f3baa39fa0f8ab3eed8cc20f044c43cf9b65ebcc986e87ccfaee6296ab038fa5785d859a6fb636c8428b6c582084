erCardOwnerIsInvalid = isEmpty(enterCardOwner);\n    const enterCardDateIsInvalid = isEmpty(enterCardDate);\n    const enterSaftyNumberIsInvalid = !isThreeChart(enterSaftyNumber);\n    const enterSendWayIsInvalid = isEmpty(enterSendWay);\n    const enterSendAddrIsInvalid = isEmpty(enterSendAddr);\n    const enterSendNameIsInvalid = isEmpty(enterSendName);\n    const enterSendPhoneIsInvalid = isEmpty(enterSendPhone);\n    const enterRecieptTypeIsInvalid = isEmpty(enterRecieptType);\n    const enterRecieptIsInvalid = isEmpty(enterReciept);\n\n    setFormIsInvalid({\n      name: enterNameIsInvalid,\n      email: enterEmailIsInvalid,\n      phone: enterPhoneIsInvalid,\n      cardNumber: enterCardNumberIsInvalid,\n      cardOwner: enterCardOwnerIsInvalid,\n      cardDate: enterCardDateIsInvalid,\n      saftyNumber: enterSaftyNumberIsInvalid,\n      sendWay: enterSendWayIsInvalid,\n      sendAddr: enterSendAddrIsInvalid,\n      sendName: enterSendNameIsInvalid,\n      sendPhone: enterSendPhoneIsInvalid,\n      recieptType: enterRecieptTypeIsInvalid,\n      reciept: enterRecieptIsInvalid,\n    });\n\n    formIsValid =\n      !enterNameIsInvalid &&\n      !enterEmailIsInvalid &&\n      !enterPhoneIsInvalid &&\n      !enterCardNumberIsInvalid &&\n      !enterCardOwnerIsInvalid &&\n      !enterCardDateIsInvalid &&\n      !enterSaftyNumberIsInvalid &&\n      !enterSendWayIsInvalid &&\n      !enterSendAddrIsInvalid &&\n      !enterSendNameIsInvalid &&\n      !enterSendPhoneIsInvalid &&\n      !enterRecieptTypeIsInvalid &&\n      !enterRecieptIsInvalid;\n\n      if (!formIsValid) {\n        return;\n      }\n\n      handleClickAlertOpen()\n\n  }\n\n  const onSubmitHandler = (event) => {\n   \n    if (!formIsValid) {\n      return;\n    }\n    const requestData = {\n      url: \"https://react-http-3d73c-default-rtdb.firebaseio.com/orangeorder.json\",\n      method: \"POST\",\n      body: {\n        enterName,\n        enterEmail,\n        enterPhone,\n        enterCardNumber,\n        enterCardOwner,\n        enterCardDate,\n        enterSaftyNumber,\n        enterSendWay,\n        enterSendAddr,\n        enterSendName,\n        enterSendPhone,\n        enterRecieptType,\n        enterReciept,\n      },\n    };\n\n    const applyData = (responseData) => {\n      console.log(requestData)\n    };\n\n    sendRequest(requestData ,applyData);\n    removeFromCartHandler()\n    handleAlertClose()\n    router.push('/');\n  };\n\n  return (\n    <div className={classes.container}>\n    <Alert openAlert={alertOpen} handleClose={handleAlertClose} handleAgree={onSubmitHandler} title='送出訂單'  alertContent='送出後無法修改，確定要送出訂單嗎？' disagree={true}/>\n      <Banner title=\"訂單確認\" />\n      {cart.items.length === 0 ? (\n        <div className={classes.wrapper}>\n          <div className={classes.cartEmptyContainer}>\n            <ShoppingCartIcon style={{ fontSize: 150, color: \"gray\" }} />\n            <br />\n            <Link href=\"/product-list\">\n              <GreenButton title=\"繼續購物\" width=\"200px\" />\n            </Link>\n          </div>\n        </div>\n      ) : (\n        <div className={classes.wrapper}>\n          <div className={classes.cartContainer}>\n            <div className={classes.cartHeader} onClick={handleExpandClick}>\n              <h2>合計: NT$ {cart.totalPrice}</h2>\n              <KeyboardArrowDownIcon />\n            </div>\n            <Collapse\n              style={{ flex: \"100%\" }}\n              in={expanded}\n              timeout=\"auto\"\n              unmountOnExit\n            >\n              <div className={classes.column}>\n                <div className={classes.cartMainPart}>商品資料</div>\n                <div className={classes.cartInfor}>單件價格</div>\n                <div className={classes.cartInfor}>數量</div>\n                <div className={classes.cartInfor}>小計</div>\n              </div>\n              {cart.items.map((item) => (\n                <div className={classes.column}>\n                  <div className={classes.cartMainPart}>\n                    <img src={item.img} />\n                    <p>{item.name}</p>\n                  </div>\n                  <div className={classes.cartInfor}>NT$ {item.price}</div>\n                  <div className={classes.cartInfor}>{item.quantity}</div>\n                  <div className={classes.cartInfor}>NT$ {item.totalPrice}</div>\n                </div>\n              ))}\n            </Collapse>\n          </div>\n          <div className={classes.custinforContainer}>\n            <div>\n              <div className={classes.custHeaderColumn}>\n                <div>顧客資訊</div>\n              </div>\n              <div className={classes.column}>\n                <div className={classes.custColumn}>\n                  <p>顧客姓名</p>\n                  <input defaultValue=\"鄭伊婷\" ref={name} />\n                  {isFormInvalid.nameState && <p>請輸入姓名</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>電子信箱</p>\n                  <input defaultValue=\"bz850308@gmail.com\" type=\"email\" ref={email} />\n                  {isFormInvalid.emailState && <p>請輸入Email</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>電話號碼</p>\n                  <input ref={phone} maxLength=\"10\" />\n                  {isFormInvalid.phoneState && <p>請輸入電話</p>}\n                </div>\n              </div>\n            </div>\n          </div>\n          <div className={classes.custinforContainer}>\n            <div>\n              <div className={classes.custHeaderColumn}>\n                <div>付款資料</div>\n              </div>\n              <div className={classes.column}>\n                <div className={classes.custColumn}>\n                  <p>卡號</p>\n                  <input type=\"number\" ref={cardNumber} />\n                  {isFormInvalid.cardNumberState && <p>請輸入卡號</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>持卡人姓名</p>\n                  <input ref={cardOwner} />\n                  {isFormInvalid.cardOwnerState && <p>請輸入持卡人姓名</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>有效日期(MM/YY)</p>\n                  <input ref={cardDate} />\n                  {isFormInvalid.cardDateStateState && <p>請輸入有效日期</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>安全碼</p>\n                  <input ref={saftyNumber} maxLength=\"3\" />\n                  {isFormInvalid.saftyNumberState && <p>請輸入安全碼</p>}\n                </div>\n              </div>\n            </div>\n            <div>\n              <div className={classes.custHeaderColumn}>\n                <div>送貨資料</div>\n              </div>\n              <div className={classes.column}>\n                <div className={classes.custColumn}>\n                  <p>收件人名稱</p>\n                  <input defaultValue=\"鄭伊婷\" ref={sendName} />\n                  {isFormInvalid.sendNameState && <p>請輸入收件人名稱</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>收件人電話號碼</p>\n                  <input ref={sendPhone} maxLength=\"10\" />\n                  {isFormInvalid.sendPhoneState && <p>請輸入收件人電話號碼</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>送貨方式</p>\n                  <input defaultValue=\"宅配\" ref={sendWay} />\n                  {isFormInvalid.sendWayState && <p>請輸入送貨方式</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>送貨地址</p>\n                  <input ref={sendAddr} />\n                  {isFormInvalid.sendAddrState && <p>請輸入送貨地址</p>}\n                </div>\n              </div>\n            </div>\n            <div>\n              <div className={classes.custHeaderColumn}>\n                <div>發票資訊</div>\n              </div>\n              <div className={classes.column}>\n                <div className={classes.custColumn}>\n                  <p>發票類型</p>\n                  <select ref={recieptType} defaultValue='雲端發票'>\n                    <option>雲端發票</option>\n                  </select>\n                  {isFormInvalid.recieptTypeState && <p>請選擇雲端發票</p>}\n                </div>\n                <div className={classes.custColumn}>\n                  <p>輸入載具</p>\n                  <input ref={reciept} />\n                  {isFormInvalid.recieptState && <p>請輸入載具</p>}\n                </div>\n              </div>\n            </div>\n          </div>\n          <div className={classes.bottomContainer}>\n            <GreenButton\n              title=\"送出訂單\"\n              width=\"200px\"\n              onClick={comfirmDataHandler}\n            />\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default index;\n"]},"metadata":{},"sourceType":"module"}